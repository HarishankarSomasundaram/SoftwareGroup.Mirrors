        <param name="ConversionPattern" value=" %d [%t] %-5p %C - %M &lt;&gt;  %m%n%n" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="rollingFile" />
    </root>
  </log4net>
